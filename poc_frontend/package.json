{
  "name": "angular-poc",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "extract-en-translations": "ng2-translate-extract --dir ./src --output ./src/app/i18n/en.json --format=json --clean",
    "extract-fr-translations": "ng2-translate-extract --dir ./src --output ./src/app/i18n/fr.json --format=json --clean",
    "extract-nl-translations": "ng2-translate-extract --dir ./src --output ./src/app/i18n/nl.json --format=json --clean",
    "extract": "npm run extract-en-translations && npm run extract-fr-translations && npm run extract-nl-translations",
    "synchroniseTranslations": "synchroniseTranslations --dir ./src --translationFolder ./src/app/i18n",
    "harcodedTexts":"i18n-lint -t '{{,}}' src/app/**/*.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@ciortanmadalina/angular2-translations-synchroniser": "^1.0.5",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "ng2-translate": "^4.2.0",
    "primeng": "1.1.2",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.4.0",
    "@biesbjerg/ng2-translate-extract": "^0.3.5",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.22-1",
    "codelyzer": "~2.0.0-beta.1",
    "i18n-lint": "github:jwarby/i18n-lint",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "^4.1.1",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
